   "source": [
    "# while문을 이용하여 1~100까지의 정수 중 3의 배수의 합을 출력"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "7b9116b6-b631-408d-8fdb-a1e2973acacb",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1~100 중 3의 배수의 합은 1683이다\n"
     ]
    }
   ],
   "source": [
    "n = 1\n",
    "total = 0\n",
    "\n",
    "while n < 101:\n",
    "    if n % 3 == 0:\n",
    "        total += n\n",
    "    n+=1\n",
    "    \n",
    "# while n*3 < 101:\n",
    "#     sum += n*3\n",
    "#     n+=1\n",
    "\n",
    "print(\"1~100 중 3의 배수의 합은 {}이다\".format(total))"
   ]
